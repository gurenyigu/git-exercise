        { name: 'JavaScript', value: 'js' },
        { name: 'Objective C', value: 'oc' },
        { name: 'PHP', value: 'php' },
        { name: 'Python', value: 'python' },
        { name: 'Ruby', value: 'ruby' },
        { name: 'SQL', value: 'sql'}
    ],
    tabIndent : true
});
//创建话题
$('#createDiscussionUse').click(function(){
    var title = $.trim(eval(document.getElementById('discussionTitle')).value);
    var content = $.trim(editor.getValue());
    if(title.length>30){
        $('.error_msg' ).text("标题太长，请输入30个字以内的标题！");
        return;
        }
    if(content.length>5000){
        $('.error_msg' ).text("内容过长！");
        return;
    }
    if((content == '') || (title == '') || ($ ( "#discussionTextDialogForm input[name='tags']" ).attr("value") == '')){
        $('.error_msg' ).text("标题、内容和标签都不能是空的！");
        return;
    }
    $("#discussionTextDialogForm input[name='content']" ).val(editor.getValue());
    $("#discussionTextDialogForm input[name='title']" ).val(eval(document.getElementById('discussionTitle')).value);
    var ajax_url = $("#discussionTextDialogForm").attr('action');
    var ajax_type = $("#discussionTextDialogForm").attr('method');
$.ajax({
type:ajax_type,
url:ajax_url,
data:{

<script>
    $(document ).ready(function(){
        var searchFunc = function(condition){
            if(condition!=null && condition!=""){
                var data = encodeURIComponent(condition);
                window.location.replace("/search_1.html?condition=" + data,"_blank");
            }else{
                return;
            }
        }
        //搜索按钮点击
        $(".search_btn" ).bind("click",function(){
            var condition = $("#condition" ).val( ).trim();
            searchFunc(condition);
        });
        //整个页面的回车监听
        $(document ).keyup(function(e){
            var curKey = e.which;
            if(curKey==13){
                var condition = $("#condition" ).val( ).trim();
                searchFunc(condition);
            }
        })
        //搜索框的回车监听
        $("#condition" ).keyup(function(e){
            var curKey = e.which;
            if(curKey==13){
                $(".search_btn" ).click();
            }
        });
    })
</script>
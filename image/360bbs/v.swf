<script>
    $(function(){
        $ ("#codeImg").click(function (){
            $(this).attr("src","/captcha?date"+new Date( ).getTime());
        });
        $(".close_resume_window" ).click(function(){
            $("#verificationWindow" ).hide();
        });
        $("#cancelBtn" ).click(function(){
            $("#verificationWindow" ).hide();
        });
        $("#verificationBtn" ).click(function(){
            var phone = d.r.ele.phone.val ( ) ;
            var code=$("#captCode" ).val();

            if(code==null || code==""){
                $("#errorMsg" ).html("<i class='fa fa-times-circle'></i>请输入验证码").show();
                return;
            }
            d.c.getCode ( phone , code) ;
            
        });
        $("#verificationBtn2" ).click(function(){
            var code=$("#captCode" ).val();
            if(code==null || code==""){
                $("#errorMsg" ).html("<i class='fa fa-times-circle'></i>请输入验证码").show();
                return;
            }
            $.ajax({
                url:"/code_verification/"+code,
                type:"get",
                success:function(data){
                    if(data=="0"){
                        $("#errorMsg" ).html("<i class='fa fa-times-circle'></i>请输入验证码").show();
                    }
                    if(data=="-1"){
                        $("#errorMsg" ).html("<i class='fa fa-times-circle'></i>验证码错误").show();
                        $("#codeImg").click();
                    }
                    if(data=="1"){
                        $("#errorMsg" ).hide();
                        $(".form_error" ).hide();
                        $("#vcode" ).val(code);
                        $("#verificationWindow" ).hide();
                    }
                }
            });
        });
    })

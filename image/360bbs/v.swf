    $("#createPostTextDialogForm input[name='content']" ).val(editor.getValue());
    $("#createPostTextDialogForm input[name='discussionid']" ).val($('#createpost' ).attr("value"));
    $("#createPostTextDialogForm" ).submit();
})
//编辑回复
$('#editPostUse').click(function(){

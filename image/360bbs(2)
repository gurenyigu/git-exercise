    dfsj_jq("a.share_qqone").click(function(){
        var title = sharedec;
        var rLink = shareurl;
        var summary=sharedec;
        var site = "360社区";
        var pic="";
        shareQzone(title, rLink, summary, site, pic);
    });

    
    var qqshare=function() {
        var p = {
            url:shareurl,
            desc: sharedec,
            title: sharedec,
            summary:sharedec,
            pics: "",
            site: '360社区',
        };
        var s = [];
        for (var i in p) {
            s.push(i + '=' + encodeURIComponent(p[i] || ''));
        }
        dfsj_jq(".share_qq").attr("href","http://connect.qq.com/widget/shareqq/index.html?"+s.join('&'));
    };

    function shareQzone(title, url, summary, site, picurl) {
        var top = window.screen.height / 2 - 250;
        var left = window.screen.width / 2 - 300;
        var shareqqzonestring='http://sns.qzone.qq.com/cgi-bin/qzshare/cgi_qzshare_onekey?title='+title+'&url='+encodeURIComponent(url)+'&summary='+summary+'&site='+site+'&pics='+encodeURIComponent(picurl);
        window.open(shareqqzonestring, 'newwindow', 'scrollbars=no,width=600,height=450,left=' + left + ',top=' + top + ',status=no,resizable=yes');
    }

</script>


    dfsj_jq(".stable-box").click(function(){
        dfsj_jq(".stable-box").css("color","#2ec7bb").css("border-bottom","1px solid #2ec7bb");
        dfsj_jq(".test-box").css("color","#3c3c3c").css("border-bottom","none");
        dfsj_jq(".update-test").hide();
        dfsj_jq(".update-stable").show();
    });

    dfsj_jq(".test-box").click(function(){
        dfsj_jq(".test-box").css("color","#2ec7bb").css("border-bottom","1px solid #2ec7bb");
        dfsj_jq(".stable-box").css("color","#3c3c3c").css("border-bottom","none");
        dfsj_jq(".update-stable").hide();
        dfsj_jq(".update-test").show();
    });
//热门推荐和最新主贴切换
dfsj_jq(".hot-posts").click(function(){
dfsj_jq(".hot-posts").addClass("posts-selected");
dfsj_jq(".new-posts").removeClass("posts-selected");
dfsj_jq(".hot-posts-pannel").show();
dfsj_jq(".new-posts-pannel").hide();
});
dfsj_jq(".new-posts").click(function(){
dfsj_jq(".new-posts").addClass("posts-selected");
dfsj_jq(".hot-posts").removeClass("posts-selected");
dfsj_jq(".hot-posts-pannel").hide();
dfsj_jq(".new-posts-pannel").show();
});

    dfsj_jq(function(){
        var topColWidth=dfsj_jq(".dfsj_focus .topCol ul:last-child").width();
        var topColHalfWidth=(1225-topColWidth)/2;
        dfsj_jq(".dfsj_focus .topCol ul").css("left",topColHalfWidth);
        var getpagenum=dfsj_jq("#pagenum").attr("value");
        var pagenum;
        if(getpagenum=="page"){
            pagenum=1;
        }else{
            pagenum=parseInt(getpagenum.substr(4));
        }
        var pagecontent="";
        for(var i=1;i<=pagenum;i++){
            pagecontent+='<a href="http://bbs.360.cn/newindex.php?page='+i+'">'+i+'</a>';
        }
        dfsj_jq(".index-pages").append(pagecontent);

        var url = location.search;
        var pagenum = url.substr(url.indexOf('=') + 1) - 1;
        if(pagenum==-1) pagenum=0;
        dfsj_jq(".index-pages>a:eq("+pagenum+")").addClass("index-pages-select");


    });

    var week; 
    var d = new Date();
    var day = d.getDay();
    if(day==0) week="星期日";
    if(day==1) week="星期一";
    if(day==2) week="星期二";
    if(day==3) week="星期三";
    if(day==4) week="星期四";
    if(day==5) week="星期五";
    if(day==6) week="星期六";

    var vMon = d.getMonth() + 1;
    var vDay = d.getDate();
    var days=vMon+"."+vDay;
    dfsj_jq(".right-week").text(week);
    dfsj_jq(".right-day").text(days);
    dfsj_jq(".right-signnums").text(8140);



    var shareurl="";
    var sharedec="";
//给微博分享加上链接
dfsj_jq(".share_xinlang").each(function(){
var xinlangTitle=dfsj_jq(this).parent().parent().parent().parent().find(".new-title a").text();
var xinlangUrl=dfsj_jq(this).parent().parent().parent().parent().find(".new-title a").attr("href");
dfsj_jq(this).attr("href","http://v.t.sina.com.cn/share/share.php?url=http://bbs.360.cn/"+encodeURIComponent(xinlangUrl)+"&appkey=1874498659&title="+xinlangTitle+"——来自@360社区");
});

    dfsj_jq(".news-share").hover(function(){
        dfsj_jq(".share-index").hide();
        dfsj_jq(this).parent().next().show();
        sharedec=dfsj_jq(this).parent().parent().prev().prev().children().text();
        shareurl="http://bbs.360.cn/"+dfsj_jq(this).parent().parent().prev().prev().children().attr("href");
        qqshare();
    });

    dfsj_jq(".share-index").hover(function(){
        dfsj_jq(this).show();
    },function(){
        dfsj_jq(".share-index").hide();
    });

    window.onscroll = function() {
        dfsj_jq(".share-index").hide();
    };

    window._bd_share_config = {
